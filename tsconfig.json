{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": ["jest"],
    "baseUrl": ".",
    "paths": {
      "@api": ["src/api"],
      "@api/*": ["src/api/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@theme": ["src/theme"],
      "@theme/*": ["src/theme/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@shared": ["src/shared"],
      "@shared/*": ["src/shared/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/Pods"]
}
